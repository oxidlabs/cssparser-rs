{
    ".center-block": { "top": "25px" },
    ".container": {
        "position": "relative",
        "height": "100px",
        "width": "100px",
        "border": "solid",
        "display": "inline-block",
        "margin-right": "5px"
    },
    ".grid": {
        "align-items": "center",
        "display": "grid",
        "height": "100%",
        "justify-items": "center",
        "width": "100%"
    },
    ".flex": {
        "align-items": "center",
        "width": "100%",
        "display": "flex",
        "height": "100%"
    },
    ".abs": {
        "position": "absolute",
        "height": "50px",
        "background": "purple",
        "width": "50px"
    },
    ".top": { "top": "0" },
    ".center-inline": { "left": "25px" },
    ".left": { "left": "0" }
}
